<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="455.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.Controller">
   <children>
      <VBox prefHeight="455.0" prefWidth="800.0">
         <children>
            <TableView fx:id="studentTable" prefHeight="444.0" prefWidth="800.0">
              <columns>
                <TableColumn fx:id="lastNameCol" prefWidth="160.0" text="Last Name" />
                <TableColumn fx:id="firstNameCol" prefWidth="160.0" text="First Name" />
                  <TableColumn fx:id="departmentCol" prefWidth="160.0" text="Department" />
                  <TableColumn fx:id="emailCol" prefWidth="160.0" text="Email" />
                  <TableColumn fx:id="addressCol" prefWidth="160.0" text="Address" />
              </columns>
            </TableView>
            <HBox prefHeight="0.0" prefWidth="839.0" style="-fx-spacing: 70;">
               <children>
                  <Button mnemonicParsing="false" onAction="#removeButton" style="-fx-background-color: #334ff4; -fx-background-radius: 10;" text="Remove" textAlignment="CENTER" textFill="WHITE">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
                  <Button layoutX="10.0" layoutY="10.0" mnemonicParsing="false" style="-fx-background-color: #334ff4; -fx-background-radius: 10;" text="Change" textAlignment="CENTER" textFill="WHITE">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Button>
               </children>
            </HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
